pragma solidity ^0.8.0;

contract InheritanceSubmission {
    address public salesPerson;
    address public engineeringManager;

    constructor(address _salesPerson, address _engineeringManager) {
        salesPerson = _salesPerson;
        engineeringManager = _engineeringManager;
    }
}

// Deploy the Salesperson contract
// Constructor arguments: 55555, 12345, 20

// Deploy the EngineeringManager contract
// Constructor arguments: 54321, 11111, 200000

// After deploying these contracts, you'll get their addresses. Use these addresses to deploy this contract
